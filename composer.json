{
    "name": "hakito/cakephp-stripe-payment-intents-plugin",
    "type": "cakephp-plugin",
    "description": "CakePHP Plugin for Stripe Payment Intents",
    "homepage": "https://github.com/hakito/CakePHP-StripePaymentIntents-Plugin",
    "keywords": ["stripe", "payments", "payments intents", "credit card"],
    "license": "GPL-2.0",
    "authors": [
        {
            "name": "Gerd Katzenbeisser",
            "homepage": "https://github.com/hakito/CakePHP-StripePaymentIntents-Plugin/graphs/contributors"
        }
    ],
    "autoload": {
        "psr-4": {
            "StripePaymentIntents\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "StripePaymentIntents\\Test\\": "tests/"
        }
    },       
    "require": {
        "cakephp/cakephp": "^3.7",
        "stripe/stripe-php": "^7.12"
    },
    "extra": {
        "installer-name": "StripePaymentIntents"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0"
    }
}
